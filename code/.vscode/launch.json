    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "wsgi.py",
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "0"
            },
            "args": [
                "run",
                "--no-debugger"
            ],
            "jinja": true
        },
        {
            "name": "Cli Load",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "args": ["database", "load"]
        },
        {
            "name": "Pytest",
            "type": "python",
            "request": "launch",
            "module": "pytest"
        }
    ]
}